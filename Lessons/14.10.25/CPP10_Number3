#include <iostream>

template <typename T>
T sumArray(const T* arr, int size) {
    T sum = 0;
    for (int i = 0; i < size; ++i) {
        sum += arr[i];
    }
    return sum;
}

int main() {
    int arr[] = {1, 2, 3, 4, 5};
    std::cout << sumArray(arr, 5) << "\n";

    double arrD[] = {1.1, 2.2, 3.3};
    std::cout << sumArray(arrD, 3) << "\n";
}