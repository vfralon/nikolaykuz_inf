#include <iostream>

bool isPrime(int n) {
    if (n < 2) return false;
    for (int i = 2; i * i <= n; ++i) {
        if (n % i == 0) return false;
    }
    return true;
}

int main() {
    int N;
    std::cout << "Введите N: ";
    std::cin >> N;

    std::cout << "Простые числа не превышающие " << N << ": ";
    for (int i = 2; i <= N; ++i) {
        if (isPrime(i)) std::cout << i << " ";
    }
    std::cout << "\n";
}