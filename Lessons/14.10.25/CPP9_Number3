#include <iostream>

long long power(long long a, int n) {
    if (n == 0) return 1;
    return a * power(a, n - 1);
}

int main() {
    long long a;
    int n;
    std::cout << "Введите а: ";
    std::cin >> a;
    std::cout << "Введите n: ";
    std::cin >> n;

    std::cout << "Результат: " << power(a, n) << "\n";
}