#include <iostream>
#include <iomanip>
#include <string>
#include <vector>
using namespace std;

struct fr {
    string name;
    string sname;
    double a;
    double b;
    double c;
};
    
int main() {
    int n;
    cin >> n;
    double sum1 = 0, sum2 = 0, sum3 = 0;
    
    vector<fr> frnds(n);
    
    for (int i = 0; i < n; i++) {
        cin >> frnds[i].name >> frnds[i].sname >> frnds[i].a >> frnds[i].b >> frnds[i].c;
    }
    
    for (int i = 0; i < n; i++) {
        sum1 += frnds[i].a;
        sum2 += frnds[i].b;
        sum3 += frnds[i].c;
    }
    
    cout << fixed << setprecision(1);
    cout << sum1 / n << " " << sum2 / n << " " << sum3 / n << endl;
}
