#include <iostream>
#include <cstdlib>

int main() {
    int n;
    int m;
    std::cout << "Размер массива? N и M" << "\n";
    std::cin >> n >> m;
    int mas[n][m];
    int c = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            mas[i][j] = rand()%10;
            c = c + mas[i][j];
        }
    }
    std::cout << "Сумма элементов:" << c;
}